<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.autentic">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icone"
        android:label="Velcar Operador"
        android:roundIcon="@mipmap/icone_foreground"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.FinalizaOrcamento"></activity>
        <activity android:name=".Activity.CarroLateralEsquerda" />
        <activity android:name=".Activity.CarroLateralDireita" />
        <activity android:name=".Activity.CarroTraseira" />
        <activity android:name=".Activity.CarroFrente" />
        <activity android:name=".Activity.PecasDefeito" />
        <activity android:name=".Activity.CadastroVeiculo" />
        <activity android:name=".Activity.Qrcode" />
        <activity android:name=".Activity.MainActivity" />
        <activity android:name=".Activity.Cadastro" />
        <activity android:name=".Activity.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>